@{
    ViewBag.Title = "About";
    
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>User Agent: @(Context.Request.Headers[DotNetCoreOpen.PrenderMiddleware.Constants.HttpHeader_ContentType])</p>
<br />
<p class="timeout-target">Default Text.</p>
@section scripts{
    <script>
    $(function () {
        setTimeout(function () {
            $(".timeout-target").html("Use this area to provide additional information: "+ new Date());
        },1000);
    });
    </script>
}